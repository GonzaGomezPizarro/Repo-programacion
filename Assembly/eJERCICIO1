		;Practico	5
		;Ejercicio	1
		MOV		R1, #20
		MOV		R2, #1 ; PARA VER SI EL ULTIMO DIGITO TIENE 1
		MOV		R4, #0 ; ESTE VA A SER EL CONTADOR
LOOP
		AND		R3,R1,R2 ;EN R3 SE GUARDA 1 SI EL ULTIMO DIGITO ES 1 Y 0 SI NO LO ES
		CMP		R3,R2 ; SE COMPARA SI TIENE UN UNO O NO
		ADDEQ	R4,R4,#1 ; SE SUMA 1 EN EL CONTADOR SI EL ULTIMO DIGITO ES 1
		SUB		R1,R1,#2
		CMP		R1,#0
		BNE		LOOP
		
		
		
		
		END
