		;		Pasar parametro x funcion...
		;		if		(a==0) func(131); =
		CMP		R0, #0
		MOVEQ	R0, #1 ; se suele usar R0 para pasr parametros
		BLEQ		func ; Branch- link -equal
func
		;		...
		;		cuerpo de la funcion
		
		
		;		If (a==4 || a==10) x=0;
		CMP		R0,#4
		CMPNE	R0,#10
		MOVEQ	R1,#0
		;
		;
		;
		;		ROL R0,#1  =  ROR R0,#31  =
		MOV		R0,R0,ROR #31 ; MULTIPLICAR X 2
		;
		MOV		R0,R0,ROR #1 ; DIVIDIR X 2
		
		
		
		END
		
